{
  "parser": "babel-eslint",
  "extends": ["airbnb","prettier"],
  "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/href-no-hash": "off",
    "prettier/prettier": ["error", { "singleQuote": true}]
  },
  "env": {
    "jest": true
  }
}

